{
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "support.type.property-name.table",
        "settings": { "foreground": "#4AB68C" }
      }
    ]
  },
  "[fish]": {
    "editor.defaultFormatter": "bmalehorn.vscode-fish"
  },
  "[markdown]": {
    "editor.wordWrap": "off"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.fixAll.ruff": true,
      "source.organizeImports.ruff": true
    }
  },
  "[javascript]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": [
      "source.fixAll.eslint",
      "source.formatDocument"
    ]
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "files.associations": {
    "*.css": "tailwindcss",
    "*.svg": "html"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "search.exclude": {
    "**/var/folders/**": true
  },
  "workbench.colorCustomizations": {
    "tree.indentGuidesStroke": "#ffb86c"
  },
  "tailwindCSS.experimental.classRegex": [
    ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSurroundingLines": 15,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.lightbulb.enabled": false,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderFinalNewline": "off",
  "editor.renderWhitespace": "trailing",
  "editor.scrollBeyondLastLine": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 100,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.inputValidationSubjectLength": 72,
  "path-autocomplete.extensionOnImport": true,
  "path-autocomplete.includeExtension": true,
  "path-autocomplete.pathMappings": { "/": "/" },
  "path-autocomplete.triggerOutsideStrings": true,
  "python.defaultInterpreterPath": "venv/bin/python",
  "ruff.path": ["venv/bin/ruff"],
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "error",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "manual",
  "update.showReleaseNotes": false,
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Dracula",
  "workbench.enableExperiments": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always"
}
